String [] nomes = {"Jonnathan", "Marcela", "Daiane", "Keren", "Cris", "Matheus", "Nicolly"};
int [] idades = {1, 2, 3, 4, 5, 6, 7, 8, 9};
boolean [] estados = {true, true, false, true, false, true, false};

//criando uma matriz
int [] numeros;
numeros = new int[6];
numeros[0] = 10;
numeros[1] = 20;
numeros[2] = 30;
numeros[3] = 40;
numeros[4] = 50;
