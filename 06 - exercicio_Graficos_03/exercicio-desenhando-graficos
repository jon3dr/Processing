println(nomes[3]);
println(idades[idades.length-1]);
println();


String[] nomesD = {"Felpudo","Fofura", "Lesmo", "Bugado", "Uruca", "Peluche"};

for (int i=0; i<nomesD.length; i++){
  println(nomesD[i]);
}

String [][] cidades = { 
  {"São Paulo", "São José", "Curitiba", "Belo Horizonte"},
  {"Nova York", "Los Angeles", "Bogotá", "Madrid"},
  {"Patópolis", "Cucamonga", "Neverland", "Transilvânia"}
};
println();
println();


for (int i=0; i<cidades.length; i++){ // percorrendo matriz
  for (int j=0; j<cidades[i].length; j++){
    println(cidades[i][j]);
  }
}
