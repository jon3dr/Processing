color[] cores = {#3542AF,#FFB74B,#92E857,#FF6483,#FFFFFF};

void settings() {
  size(720,486);
  println("Ol√°");
}
void setup() {
  background(#BFEDFF);
  frameRate(12);
  println("SETUP");
}
void draw() {
  fill (cores[1]);
  stroke (cores[0]);
  strokeWeight(6);
  rect(10,10,150,150,12);

  fill(cores[0]);
  stroke (cores[3]);
  strokeWeight(6);
  triangle(200,170,300,30,400,170);
}
